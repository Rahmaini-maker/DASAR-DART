main() {
  print('Hello Word');
}
main(){
    // membuat dasar dart dengan tipe data
    String nama = "Rahma Ini Sihotang";
    int umur = 20;
    double tinggi = 160;
    bool isMenikah = false;

    // membuat dasar dart dengan kata kunci var
    var alamat = "Medan, Indonesia";

    // mencetak variabel
    print("Nama saya $nama. Umur $umur tahun. Tinggi sekitar $tinggi cm.");
    print("Menikah: $isMenikah");
    print("Alamat: $alamat");
}
